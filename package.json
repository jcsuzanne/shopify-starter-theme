{
  "name": "jcs-shopify-starter-theme",
  "description": "Starter theme for Shopify using modern dev tools like Vite.js, TailwindCSS and with Pieces.js (for native webcomponents)",
  "author": "JC SUZANNE (https://github.com/jcsuzanne)",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=14.17",
    "npm": ">=6.0"
  },
  "scripts": {
    "dev": "concurrently --raw --kill-others --passthrough-arguments \"npm run shop:start {@}\" \"npm run vite:watch\" --",
    "vite:watch": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "shop:start": "shopify theme dev --poll --live-reload=\"hot-reload\" --notify=/tmp/theme.update -e development"
  },
  "dependencies": {
    "@swup/fade-theme": "^2.0.1",
    "@swup/parallel-plugin": "^0.4.0",
    "@swup/preload-plugin": "^3.2.10",
    "@swup/scroll-plugin": "^3.3.2",
    "gsap": "^3.12.5",
    "lenis": "^1.1.10",
    "piecesjs": "^0.2.0",
    "swup": "*"
  },
  "devDependencies": {
    "@by-association-only/vite-plugin-shopify-clean": "^2.0.0",
    "@csstools/postcss-global-data": "^3.0.0",
    "@shopify/prettier-plugin-liquid": "1.5.0",
    "@splidejs/splide": "^4.1.4",
    "@swup/fragment-plugin": "^1.1.1",
    "@swup/progress-plugin": "^3.2.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "0.5.14",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "alpinejs": "^3.14.1",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.4",
    "concat": "^1.0.3",
    "concurrently": "^8.2.2",
    "cssnano": "6.1.2",
    "currency.js": "^2.0.4",
    "events": "^3.3.0",
    "mobile-detect": "^1.4.5",
    "path": "^0.12.7",
    "postcss": "^8.4.41",
    "postcss-calc": "^10.0.2",
    "postcss-cli": "^11.0.0",
    "postcss-comment": "^2.0.0",
    "postcss-conditionals": "^2.1.0",
    "postcss-custom-media": "^11.0.1",
    "postcss-extend-rule": "^4.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-for": "^2.1.1",
    "postcss-import": "^16.1.0",
    "postcss-import-ext-glob": "^2.1.1",
    "postcss-mixins": "^10.0.1",
    "postcss-nested": "^6.2.0",
    "postcss-pxtorem": "^6.1.0",
    "postcss-simple-vars": "^7.0.1",
    "postcss-utilities": "^0.8.4",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwind-clip-path": "^1.0.0",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.10",
    "tailwindcss-easing": "^0.2.1",
    "tailwindcss-scoped-preflight": "^3.4.3",
    "vite": "^5.4.2",
    "vite-plugin-page-reload": "^0.2.0",
    "vite-plugin-shopify": "^3.0.1"
  }
}
